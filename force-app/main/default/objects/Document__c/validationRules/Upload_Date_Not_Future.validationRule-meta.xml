<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Upload_Date_Not_Future</fullName>
    <active>true</active>
    <description>Ensures upload date cannot be in the future</description>
    <errorConditionFormula>Upload_Date__c &gt; NOW()</errorConditionFormula>
    <errorDisplayField>Upload_Date__c</errorDisplayField>
    <errorMessage>Upload date cannot be in the future</errorMessage>
</ValidationRule>